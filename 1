# 定义个人信息
name = "wuliancheng"  # 你的名字
phone_number = "+82 010 8069 3126"  # 你的电话号码

# 输出并模拟打印功能
def print_personal_info(name, phone):
    print("正在打印个人信息...")
    print("-------------------")
    print(f"姓名：{name}")
    print(f"电话号码：{phone}")
    print("-------------------")
    print("打印完成！")

# 调用函数执行
print_personal_info(name, phone_number)
